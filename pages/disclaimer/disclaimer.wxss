}



.content-box text{
  display: block;
  width: 100%;
  padding:100rpx 0px;
  text-align: center;
  /*font-size:100rpx;*/
}
.content-box button,.alert button{
  width:95%;
  margin: 0 auto;
  display: block;
  margin-top: 50rpx;
}

.alert{
  box-sizing: border-box;
  position:absolute;
  height:1500rpx;
  left: 0px;
  top:0px;
  width:100%;
  padding:100rpx 40rpx;
  font-size:14px;
  background: rgba(0, 0, 0, 0.7);
  min-height:100%;
  text-align: center;
  color:#fff;
}

.alert-content {
  box-sizing: border-box;
  /*position: fixed;*/
  position:absolute;
  border-radius: 3px;
  height:1500rpx;
  left: 16px;
  right: 16px;
  top:32px;
  /*width:80%;*/
  padding:20rpx 40rpx;
  font-size:14px;
  background: #fff;
  min-height:100%;
  /*text-align: center;*/
  color:#333;
}


